<script type="module">
    import QrScanner from "./qr-scanner.min.js";
    QrScanner.WORKER_PATH = "./qr-scanner-worker.min.js";

    const stream = document.getElementById("camera-stream");

    const scanner = new QrScanner(stream, result => open(result, "_blank"));

    scanner.start();
</script>
